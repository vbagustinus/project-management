<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./partials/head')%>
</head>
<body>
  <%- include('./partials/header')%>
  <!-- HERO SECTION -->
  <section class="hero is-primary">
    <div class="hero-body">
      <div class="container">
        <h1 class="title">
          Add Project
        </h1>
        <h2 class="subtitle">
          Please input your new project here
        </h2>
      </div>
    </div>
  </section>
  <!-- HERO SECTION END -->
  <!-- SECTION START -->
  <br>
  <br>
  <section class="container">
    <div class="container">
      <form class="" action="add" method="post">
        <div class="field">
          <label class="label">Project Name:</label>
          <div class="control">
            <input name="name" class="input" type="text" placeholder="Text input">
          </div>
        </div>

        <div class="field is-grouped">
          <div class="control">
            <button class="button is-primary">Submit</button>
          </div>
        </div>
      </form>
    </div>
  </section>
  <!-- SECTION END -->
  <%- include('./partials/footer')%>
</body>
</html>
